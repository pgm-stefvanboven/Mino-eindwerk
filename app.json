{
  "expo": {
    "name": "Mino",
    "slug": "mino-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "minoapp",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#09090b",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stefvanboven.minoapp"
    },
    "android": {
      "package": "com.stefvanboven.minoapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#09090b"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png",
      "description": "Mino: Slimme zorg, gerust gevoel."
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#09090b"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Mino heeft toegang nodig tot je camera om medicijnen te scannen."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}